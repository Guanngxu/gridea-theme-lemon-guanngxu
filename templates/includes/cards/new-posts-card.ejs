<div class="id_card i-card">
    <h1 class="id_card-title">
        最新文章
    </h1>
    <div class="new-aticles">
        <% site.posts.forEach(function(post, index) { %>
        <% if (index < 5) { %>
            <h3>
                <a href="<%= post.link %>"><%= post.title %></a>
            </h3>
        <% } %>
        <% }); %>
    </div>
</div>