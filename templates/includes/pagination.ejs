<% if (pagination.prev ||pagination.next) { %>
    <div class="pagination-container">
        <% if (pagination.prev) { %>
            <a href="<%= pagination.prev %>" class="page-btn btn">上一页</a>
            <% } %>
                <% if (pagination.next) { %>
                    <a href="<%= pagination.next %>" class="page-btn btn">下一页</a>
                    <% } %>
    </div>
    <% } %>